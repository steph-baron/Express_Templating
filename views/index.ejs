<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>All Pokemon</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <td>Sprite</td>
          <td>Name</td>
          <td>Pokedex Number</td>
        </tr>
      </thead>
      <tbody>
        <% for (var i = 0; i < pokemon.length; i++) { %>
        <tr>
          <td><%=pokemon[i].id %></td>
          <td><%=pokemon[i].name %></td>
          <td><img src=<%=pokemon[i].image_url%>></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
